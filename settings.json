  "security.workspace.trust.untrustedFiles": "open",
  "autoimport.useSemiColon": false,

  /***************************************************************
   * Vim
   ***************************************************************/

  "vim.normalModeKeyBindings": [
    // Use `E` to move the cursor to the end of the word, i.e. the character before the next whitespace.
    // (To go to the beginning of the next word, i.e. after the next whitespace including, use `W`.)
    // (To go to the beginning of the next punctuation, use `w`.)
    {
      "before": ["E"],
      "after": ["J"]
    },
  ],
  "vim.visualModeKeyBindings": [
    // same as in normalModeKeyBinding
    {
      "before": ["E"],
      "after": ["J"]
    },
    // remap cursor down in visual mode to use soft-wrapped lines
    {
      "before": ["n"],
      "after": ["g", "n"]
    },
    {
      "before": ["e"],
      "after": ["g", "e"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // Use 0 to go to the beginning of the first non-whitespace character instead of the first character
    {
      "before": ["0"],
      "after": ["^"]
    },
    {
      "before": ["^"],
      "after": ["0"]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    // same as in normalModeKeyBindingsNonRecursive
    {
      "before": ["0"],
      "after": ["^"]
    },
    {
      "before": ["^"],
      "after": ["0"]
    },
    // Use $ to go to the last non-whitespace character instead of the next newline. Works the same in normal mode, but works much better in visual mode.
    {
      "before": ["$"],
      "after": ["g", "_"]
    },

  ],

  "git.openRepositoryInParentFolders": "never",
  "workbench.startupEditor": "none",
  "prettier.requireConfig": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "search.exclude": {
    "**/*.js.map": true,
    "dist": true,
    "package-lock.json": true,
    "yarn.lock": true
  },
  "explorer.confirmDelete": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "window.zoomLevel": -2,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false
  },
  "window.commandCenter": false,
  "workbench.editor.empty.hint": "hidden",
  "editor.accessibilitySupport": "off",
  "terminal.integrated.tabs.enabled": false,
  "workbench.activityBar.location": "hidden",

